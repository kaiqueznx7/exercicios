programa {
  inteiro num1, num2
  funcao inicio() {
    escreva("Digite o 1° número: ")
    leia(num1)
    escreva("Digite o 2° número: ")
    leia(num2)
    

  escreva("\n","A soma dos 2 números digitados é ", Somar(num1, num2))
  }
  funcao inteiro Somar(inteiro numero1, inteiro numero2){
    retorne numero1 + numero2

  }
}
